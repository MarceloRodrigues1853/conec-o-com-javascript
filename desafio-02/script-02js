
// Capturando os elementos criados no arquivo HTML
const titulo = document.getElementById('titulo');
const link = document.querySelector('a');
const listaNaoOrdenada = document.querySelector('ul');
const listaOrdenada = document.getElementById('lista-ordenada');

// Adicionando conteúdo aos elementos usando as propriedades innerText e innerHTML
titulo.innerText = 'Título do Site';
link.innerText = 'Proz Educação';

// Adicionando itens simples na lista não ordenada
listaNaoOrdenada.innerHTML = '<li>Item 1</li><li>Item 2</li><li>Item 3</li>';

// Adicionando itens com links para outros sites na lista ordenada
listaOrdenada.innerHTML = '<li><a href="https://www.site1.com">Link 1</a></li><li><a href="https://www.site2.com">Link 2</a></li><li><a href="https://www.site3.com">Link 3</a></li>';

// Adicione logs para verificar se os elementos foram capturados corretamente
console.log(titulo);
console.log(link);
console.log(listaNaoOrdenada);
console.log(listaOrdenada);